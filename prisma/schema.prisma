generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model HistoryEntry {
  id               String   @id @default(cuid())
  inputText        String
  ownedIngredients Json
  dishName         String?
  recommendations  Json?
  recipeDetail     Json?
  createdAt        DateTime @default(now())

  @@index([createdAt])
}
